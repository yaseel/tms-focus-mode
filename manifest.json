{
  "manifest_version": 3,
  "name": "TMS Focus Mode",
  "version": "1.0",
  "description": "Toggle layout changes on ELTE TMS website for better focus and less distractions.",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
